<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>Id</th>
            <th>Titre</th>
            <th>Slug</th>
            <th></th>
        </tr>
    </thead>
    <!--tfoot>
      <tr>
        <th>Id</th>
        <th>Titre</th>
        <th>Slug</th>
        <th></th>
      </tr>
    </tfoot-->
    <tbody>
        {% for el in list %}
        <tr>
            <td>{{ el.id }}</td>
            <td>{{ el.titre }}</td>
            <td>{{ el.slug }}</td>
            <td><a href="{{ path('back_office_article_edit', {id : el.id}) }}">Edit</a></td>
        </tr>
        {% endfor %}
    </tbody>
</table>
