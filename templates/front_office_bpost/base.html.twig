{% set namespace = 'App\\Controller\\FrontOffice\\' %}
<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="">
        <meta name="author" content="CÃ©dric PUJOL">
        {% block meta %}{% endblock %}
        
        <title>{% block title %}Blog Post - Start Bootstrap Template{% endblock %}</title>
        <!-- Bootstrap core CSS -->
        <link href="{{ asset('front_office_bpost/vendor/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Custom styles for this template -->
        <link href="{{ asset('front_office_bpost/css/blog-post.css') }}" rel="stylesheet">
